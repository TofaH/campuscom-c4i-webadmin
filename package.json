{
  "private": true,
  "workspaces": [
    "packages/utilities",
    "packages/api",
    "packages/components",
    "apps/*"
  ],
  "devDependencies": {
    "rimraf": "^3.0.2",
    "husky": "^7.0.0"
  },
  "scripts": {
    "start:admin": "yarn workspace admin start",
    "start:affiliate": "yarn workspace affiliate start",
    "clean": "yarn workspace run clean",
    "clean:node_modules": "yarn workspace run clean:node_modules",
    "build": "yarn workspaces run build",
    "build:utils": "yarn workspace @packages/utilities build",
    "build:api": "yarn workspace @packages/api build",
    "build:components": "yarn workspace @packages/components build",
    "build:packages": "yarn build:utils && yarn build:api && yarn build:components",
    "build:admin": "yarn workspace admin build",
    "build:affiliate": "yarn build:utils && yarn build:api && yarn build:components && yarn workspace affiliate build",
    "lint": "yarn workspaces run lint",
    "lint:ts": "yarn workspaces run lint:ts",
    "prepare": "husky install",
    "reset": "rimraf ./**/node_modules ./**/lib ./**/yarn.lock && yarn && yarn build:packages"
  },
  "name": "hir-web-admin",
  "version": "0.0.1",
  "dependencies": {},
  "resolutions": {
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "react-error-overlay": "6.0.9"
  }
}